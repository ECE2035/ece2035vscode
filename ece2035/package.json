{
  "name": "ece2035",
  "displayName": "ECE2035",
  "description": "VS Code Companion Extension for Georgia Tech ECE 2035",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDebugResolve:mipsvm"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ece2035.helloWorld",
        "title": "Hello World"
      }
    ],
    "breakpoints": [
      {
        "language": "mips"
      }
    ],
    "debuggers": [
      {
        "type": "mipsvm",
        "languages": [
          "mips"
        ],
        "label": "MIPS Debug",
        "program": "C:\\Users\\dcoop\\github\\mipsemulator\\mipsemulator.exe",
        "runtime": "terminal",
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "A workspace-relative path to the assembly file.",
                "default": "${file}"
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "mipsvm",
            "request": "launch",
            "name": "Run Assembly",
            "program": "${file}"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "riscv",
        "aliases": [
          "RISC-V",
          "RISC-V Assembly"
        ],
        "extensions": [
          ".asm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "riscv",
        "scopeName": "source.riscv",
        "path": "./syntaxes/riscv.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
